      "StreamConfiguration": {
        "additionalProperties": false,
        "description": "\u003cp\u003eA configuration for a streaming session.\u003c/p\u003e",
        "properties": {
          "AutomaticTerminationMode": {
            "$ref": "#/definitions/AutomaticTerminationMode"
          },
