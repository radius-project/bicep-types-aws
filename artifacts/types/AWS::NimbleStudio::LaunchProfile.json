          },
          "Ec2InstanceTypes": {
            "description": "\u003cp\u003eThe EC2 instance types that users can select from when launching a streaming session\n            with this launch profile.\u003c/p\u003e",
            "items": {
              "$ref": "#/definitions/StreamingInstanceType"
            },
            "maxItems": 30,
            "minItems": 1,
