{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Jest Tests",
        "type": "node",
        "request": "launch",
        "runtimeArgs": [
          "--inspect-brk",
          "${workspaceRoot}/src/aws-type-generator/node_modules/.bin/jest",
          "--runInBand",
          "--no-cache"
        ],
        "cwd": "${workspaceFolder}/src/aws-type-generator/",
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "sourceMaps": true
      }
    ]
  }